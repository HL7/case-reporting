@prefix fhir: <http://hl7.org/fhir/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# - resource -------------------------------------------------------------------

<http://fhir.hl7.org/us/ecr/Encounter/ecr-encounter-genexample> a fhir:Encounter;
  fhir:nodeRole fhir:treeRoot;
  fhir:Resource.id [ fhir:value "ecr-encounter-genexample"];
  fhir:Encounter.identifier [
     fhir:index -1;
     fhir:Identifier.system [ fhir:value "http://www.acme.com/identifiers/patient or urn:ietf:rfc:3986 if the Identifier.value itself is a full uri" ];
     fhir:Identifier.value [ fhir:value "123456" ]
  ] .

# - ontology header ------------------------------------------------------------

<http://fhir.hl7.org/us/ecr/Encounter/ecr-encounter-genexample.ttl> a owl:Ontology;
  owl:imports fhir:fhir.ttl .

