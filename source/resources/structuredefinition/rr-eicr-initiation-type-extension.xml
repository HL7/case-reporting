<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="rr-eicr-initiation-type-extension"/>
    <meta>
        <versionId value="6"/>
        <lastUpdated value="2019-10-11T00:11:10.000-04:00"/>
    </meta>
    <url value="http://hl7.org/fhir/us/ecr/StructureDefinition/rr-initiation-type-eicr-extension"/>
    <version value="1.0.0"/>
    <name value="RR_Eicr_Initiation_Type_Extension"/>
    <title value="eICR Initiation Type Extension"/>
    <status value="active"/>
    <experimental value="false"/>
    <publisher value="HL7 Public Health Work Group"/>
    <description value="If present this Extension profile represents that the eICR for which this Reportability Response is a response, was either manually or alternately initiated by the provider. The eICR could also contain RCTC trigger codes."/>
    <fhirVersion value="4.0.0"/>
    <mapping>
        <identity value="eCR-CDA"/>
        <name value="eCR FHIR to CDA Mapping"/>
        <comment value="This table maps FHIR eCR profile elements to eICR/RR CDA template elements located in the CDA eICR IG (R2 STU2) and CDA RR IG (R2 STU1)."/>
    </mapping>
    <kind value="complex-type"/>
    <abstract value="false"/>
    <context>
        <type value="element"/>
        <expression value="Communication.payload"/>
    </context>
    <type value="Extension"/>
    <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Extension"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Extension">
            <path value="Extension"/>
            <min value="1"/>
            <max value="1"/>
            <isModifier value="false"/>
            <mapping>
                <identity value="eCR-CDA"/>
                <map value="Manually/Alternately Initiated eICR"/>
            </mapping>
        </element>
        <element id="Extension.url">
            <path value="Extension.url"/>
            <fixedUri value="http://hl7.org/fhir/us/ecr/StructureDefinition/rr-initiation-type-eicr-extension"/>
        </element>
        <element id="Extension.value[x]">
            <path value="Extension.value[x]"/>
            <type>
                <code value="code"/>
            </type>
            <binding>
                <strength value="extensible"/>
                <description value="This value set contains codes that describe alternative eICR initiation types."/>
                <valueSet value="http://hl7.org/fhir/us/ecr/ValueSet/valueset-eicr-initiation-ecr"/>
            </binding>
        </element>
    </differential>
</StructureDefinition>