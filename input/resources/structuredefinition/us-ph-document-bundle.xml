<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="us-ph-document-bundle"/>
    <meta>
        <versionId value="6"/>
        <lastUpdated value="2021-07-01T04:45:28.104+00:00"/>
